{
  "name": "@data-forge-services/react-core",
  "version": "1.0.0",
  "description": "Core React component library for Data Forge services including location, timezone, and more.",
  "main": "dist/data-forge.umd.js",
  "module": "dist/data-forge.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/data-forge.es.js",
      "require": "./dist/data-forge.umd.js"
    },
    "./dist/react-core.css": "./dist/react-core.css"
  },
  "files": [
    "dist",
    "setup.mjs",
    "README.md"
  ],
  "scripts": {
    "postinstall": "node setup.mjs",
    "build": "vite build",
    "setup": "node setup.mjs",
    "dev": "vite",
    "preview": "vite preview"
  },
  "keywords": [
    "react",
    "react-components",
    "component-library",
    "location-selector",
    "timezone-selector",
    "data-forge"
  ],
  "author": "Astra Techno",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "libphonenumber-js": "^1.10.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "vite": "^6.3.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Astra-Techno/DataForgeReactComponents.git"
  },
  "bugs": {
    "url": "https://github.com/Astra-Techno/DataForgeReactComponents/issues"
  },
  "homepage": "https://github.com/Astra-Techno/DataForgeReactComponents#readme"
}
